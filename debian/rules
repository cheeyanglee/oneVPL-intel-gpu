#!/usr/bin/make -f

%:
	dh $@  --buildsystem=cmake --parallel

override_dh_missing:
	dh_missing -- --fail-missing

override_dh_auto_configure:
	dh_auto_configure -- -DBUILD_TOOLS=ON

